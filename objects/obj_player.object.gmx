            }
        }
    }

    move_delta += 1000/(move_speed*1.6)
}
    
while(current_time - grav_delta &gt; 1000/grav_speed) {
    if vs &lt; 8 vs += 0.2
    vdir = 1
    slide = 2
    if keyboard_check(vk_down) slide=1
    if keyboard_check(vk_space) &amp;&amp; place_meeting(x, y + 1, map) vs = -0.06 * jump_speed
    if vs &lt;0 vdir = -1
    repeat(max(floor(vs), floor(0-vs))) {
        if !place_meeting(x, y + vdir, map){
            y += vdir
        }else {
            if !place_meeting(x + 1, y + vdir*slide, map){
                x += 1
                y += vdir*slide
                vs = min(vs, 3)
