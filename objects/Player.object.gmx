        <arguments>
          <argument>
            <kind>1</kind>
            <string>while(current_time - move_delta &gt; 1000/(move_speed*1.4)) {
    dir = 0
    anim = 'stand'
    if keyboard_check(vk_left) dir -= 1
    if keyboard_check(vk_right) dir += 1
    if keyboard_check(vk_shift) move_delta += 1000/(move_speed*2)
    
    if dir != 0 {
        if !pCollision(x + dir, y, map){
            x += dir
            if !pCollision(x, y + 1, map) &amp;&amp; pCollision(x, y + 2, map) y += 1
        }else{
            if !pCollision(x + dir, y + 1, map){
                x += dir
                y += 1
                move_delta += 1000/(move_speed*2)
            }else if !pCollision(x + dir, y - 1, map){
                x += dir
                y -= 1
                move_delta += 1000/(move_speed*2)
            }
        }
        image_xscale = dir
        anim = 'run'
        if keyboard_check(vk_shift) anim = 'walk'
    }
    
    if x &lt; 8 x = 8
    //if x &gt; mapGetValueDef(Map.map, 'width', 1) - 8 x = mapGetValueDef(Map.map, 'width', 1) - 8
    
    if !pCollision(x, y + 1, map) anim = 'jump'
    if skeleton_animation_get() != anim skeleton_animation_set(anim)
    event_user(0)
    move_delta += 1000/(move_speed*1.4)
}
    
while(current_time - grav_delta &gt; 1000/grav_speed) {
    if vs &lt; 8 vs += 0.2
    vdir = 1
    slide = 2
    if keyboard_check(vk_down) slide=1
    if keyboard_check(vk_space) &amp;&amp; (pCollision(x, y + 1, map) || global.debug) vs = -0.06 * jump_speed
    if vs &lt;0 vdir = -1
    repeat(max(floor(vs), floor(0-vs))) {
        if !pCollision(x, y + vdir, map){
            y += vdir
        }else {
            if !pCollision(x + 1, y + vdir*slide, map){
                x += 1
                y += vdir*slide
                vs = min(vs, 3)
            }else if !pCollision(x - 1, y + vdir*slide, map){
                x -= 1
                y += vdir*slide
                vs = min(vs, 3)
            }else {
                vs = 0
            }
        }
    }
    if y &lt; 0 y = 0
    //if y &gt; mapGetValueDef(Map.map, 'height', 1) + 200 y = 0
    
    event_user(1)
    grav_delta += 1000/grav_speed
}

while(current_time - step_delta &gt; 1000/60) {
    image_index += 20
    
    step_delta += 1000/60
}

if keyboard_check_pressed(vk_up) &amp;&amp; pCollisionPortal(x, y + 1, map) {
    portalUse(levelPropGetDef(pCollisionPortal(x, y + 1, map), "target", ""))
}
</string>
